<?xml version="1.0" encoding="UTF-8"?>

<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>typeSystemDescriptor</name>
    <description/>
    <version>1.0</version>
    <vendor/>
  <imports>
    <import location="BaseAnnotatorDescriptor.xml"/>
    <import location="TokenAnnotatorDescriptor.xml"/>
  </imports>
  <types>
    <typeDescription>
      <name>edu.cmu.test.Types.NGram</name>
      <description>This type is a combination of continuous tokens. In this case, the number of tokens can only be 1, 2 or 3.</description>
      <supertypeName>edu.cmu.test.Types.Base</supertypeName>
      <features>
        <featureDescription>
          <name>source</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>confidence</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>begin</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>end</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>nSize</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>TokenList</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.test.Types.Token</elementType>
          <multipleReferencesAllowed>true</multipleReferencesAllowed>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
