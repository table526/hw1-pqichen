<?xml version="1.0" encoding="UTF-8"?>

<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
    <name>typeSystemDescriptor</name>
    <description/>
    <version>1.0</version>
    <vendor/>
  <imports>
    <import location="BaseAnnotatorDescriptor.xml"/>
  </imports>
  <types>
    <typeDescription>
      <name>edu.cmu.test.Types.Token</name>
      <description>This type record the information of a single token in a document. Begin and end are two integers that can record the location and span of the token.</description>
      <supertypeName>edu.cmu.test.Types.Base</supertypeName>
      <features>
        <featureDescription>
          <name>source</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>confidence</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>begin</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>end</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
